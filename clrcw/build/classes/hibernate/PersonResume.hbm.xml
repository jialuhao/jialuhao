<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="model">
	<class
		name="PersonResume"
		table="person_resume"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="pk_resume"
		>
			<generator class="identity"/>
		</id>

		<property
			name="Name"
			column="name"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Sex"
			column="sex"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="Birthday"
			column="birthday"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Homeplace"
			column="homeplace"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Address"
			column="address"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Mailcode"
			column="mailcode"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Phone"
			column="phone"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="School"
			column="school"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Specialty"
			column="specialty"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="LimitYear"
			column="limit_year"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="CompLevel"
			column="comp_level"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Lang"
			column="lang"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Lang2"
			column="lang2"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Level1"
			column="level1"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Level2"
			column="level2"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="IntroSelf"
			column="intro_self"
			type="string"
			not-null="false"
		/>
		<property
			name="Version"
			column="version"
			type="java.lang.Short"
			not-null="false"
			length="5"
		/>
		<property
			name="Dr"
			column="dr"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="qzdq"
			column="qzdq"
			type="string"
			
		/>
		<many-to-one
			name="FkPersonImage"
			column="fk_person_image"
			class="PersonImage"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="User"
			column="user_id"
			class="UserInfo"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="Educate"
			column="educate_id"
			class="BdEducate"
			not-null="true"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>