<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="model">
	<class
		name="CompBespeak"
		table="comp_bespeak"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<version name="Version" column="version" type="short"/>
		
		<property
			name="UserId"
			column="user_id"
			type="string"
			not-null="true"
			length="20"
		/>
		
		
		
		<property
			name="Password"
			column="password"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="CompName"
			column="comp_name"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="CompAddr"
			column="comp_addr"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="MailPost"
			column="mail_post"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Telephone"
			column="telephone"
			type="string"
			not-null="true"
			length="15"
		/>
		<property
			name="Linkman"
			column="linkman"
			type="string"
			not-null="true"
			length="10"
		/>
		<property
			name="Mail"
			column="mail"
			type="string"
			not-null="true"
			length="30"
		/>
		<property
			name="CompStatus"
			column="comp_status"
			type="string"
			not-null="false"
			length="1"
		/>
		
		<property
			name="Dr"
			column="dr"
			type="string"
			not-null="false"
			length="1"
		/>
	

		<set name="CompInfos" inverse="true">
			<key column="id"/>
			<one-to-many class="CompInfo"/>
		</set>


	</class>	
</hibernate-mapping>