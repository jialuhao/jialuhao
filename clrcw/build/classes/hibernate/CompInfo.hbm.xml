<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="model">
	<class
		name="CompInfo"
		table="comp_info"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="pk_comp"
		>
			<generator class="identity"/>
		</id>

		<version name="Version" column="version" type="short"/>
		
		<property
			name="WorkScrop"
			column="work_scrop"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="NetAddress"
			column="net_address"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="CompIntro"
			column="comp_intro"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="ResumeStatus"
			column="resume_status"
			type="string"
			not-null="false"
			length="1"
		/>
		
		<property
			name="Dr"
			column="dr"
			type="string"
			not-null="false"
			length="1"
		/>
		<many-to-one
			name="Comp"
			column="comp_id"
			class="CompBespeak"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="Type"
			column="type_id"
			class="BdCompkind"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="Bd"
			column="bd__id"
			class="BdTrade"
			not-null="true"
		>
		</many-to-one>
	

		<set name="PublishJobInfos" inverse="true">
			<key column="pk_comp"/>
			<one-to-many class="PublishJobInfo"/>
		</set>


	</class>	
</hibernate-mapping>