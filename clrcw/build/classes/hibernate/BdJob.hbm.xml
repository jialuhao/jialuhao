<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="model">
	<class
		name="BdJob"
		table="bd_job"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="job_id"
		>
			<generator class="identity"/>
		</id>

		<version name="Version" column="version" type="short"/>
		
		<property
			name="JobName"
			column="job_name"
			type="string"
			not-null="true"
			length="20"
		/>
		
		<property
			name="Dr"
			column="dr"
			type="string"
			not-null="false"
			length="1"
		/>
	 <property name="address" column="address"></property>

		<set name="PublishJobInfos" inverse="true">
			<key column="job_id"/>
			<one-to-many class="PublishJobInfo"/>
		</set>

		<set name="ApplyJobInfos" inverse="true">
			<key column="job_id"/>
			<one-to-many class="ApplyJobInfo"/>
		</set>


	</class>	
</hibernate-mapping>