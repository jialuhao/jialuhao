<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="model">
	<class
		name="BdEducate"
		table="bd_educate"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<version name="Version" column="version" type="short" />

		<property
			name="EducateName"
			column="educate_name"
			type="string"
			not-null="true"
			length="20"
		/>
		
		<property
			name="Dr"
			column="dr"
			type="string"
			not-null="false"
			length="1"
		/>

		<set inverse="true" name="PersonResumes">
			<key column="id" />
			<one-to-many class="PersonResume" />
		</set>

		<set inverse="true" name="PublishJobInfos">
			<key column="id" />
			<one-to-many class="PublishJobInfo" />
		</set>



	</class>	
</hibernate-mapping>