<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="model">
	<class
		name="CompResume"
		table="comp_resume"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="pk_comp_resume"
		>
			<generator class="identity"/>
		</id>

		<property
			name="CompLoginId"
			column="comp_login_id"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="File"
			column="isFile"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Version"
			column="version"
			type="java.lang.Short"
			not-null="false"
			length="5"
		/>
		<property
			name="Dr"
			column="dr"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="createtime"
			column="createtime"
			type="string"
			not-null="false"
			length="1"
		/>
		<many-to-one
			name="CompPost"
			column="comp_post_id"
			class="PublishJobInfo"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="PkResume"
			column="pk_resume"
			class="PersonResume"
			not-null="true"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>