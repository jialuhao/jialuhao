<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="model">
	<class
		name="Servicelog"
		table="servicelog"
	> 
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<!-- please tell Joe Hudson that the type 'nchar' could not be resolved.. defaulting to java.lang.String -->
		<property
			name="jobid"
			column="pk_job"
			type="java.lang.String"
			not-null="false"
			length="50"
		/>
		<property
			name="losewhy"
			column="losewhy"
			type="java.lang.String"
			not-null="false"
			length="50"
		/>
		
		<property
			name="otherwhy"
			column="otherwhy"
			type="java.lang.String"
			not-null="false"
			length="100"
		/>
		<!-- please tell Joe Hudson that the type 'nchar' could not be resolved.. defaulting to java.lang.String -->
	
		<property
			name="writetime"
			column="writetime"
			type="java.lang.String"
			not-null="false"
			length="50"
		/>
		<property
			name="writeare"
			column="writeare"
			type="java.lang.String"
			not-null="false"
			length="50"
		/>
		<property
			name="writepeople"
			column="writepeople"
			type="java.lang.String"
			not-null="false"
			length="50"
		/>
		<property
			name="serviceneed"
			column="serviceneed"
			type="java.lang.String"
			not-null="false"
			length="50"
		/>
		<property
			name="opinion"
			column="opinion"
			type="java.lang.String"
			not-null="false"
			length="200"
		/>
	
		<property
			name="seasonal"
			column="seasonal"
			type="java.lang.String"
			not-null="false"
			length="50"
		/>
		
		<property
			name="number"
			column="number"
			type="java.lang.String"
			not-null="false"
			length="50"
		/>
		
<property
			name="createtime"
			column="createtime"
			type="java.lang.String"
			not-null="false"
			length="50"
		/>
	</class>	
</hibernate-mapping>