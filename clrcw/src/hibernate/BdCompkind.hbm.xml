<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="model">
	<class
		name="BdCompkind"
		table="bd_compkind"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

<version name="Version" column="version" type="short"/>

		<property
			name="CompTypeName"
			column="comp_type_name"
			type="string"
			not-null="false"
			length="10"
		/>
		
		
		
		<property
			name="Dr"
			column="dr"
			type="string"
			not-null="false"
			length="1"
		/>

		<set inverse="true" name="CompInfos">
			<key column="id" />
			<one-to-many class="CompInfo" />
		</set>



	</class>	
</hibernate-mapping>