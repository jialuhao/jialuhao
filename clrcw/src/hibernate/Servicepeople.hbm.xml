<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="model">
	<class
		name="Servicepeople"
		table="servicepeople"
	> 
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>
		<property
			name="serviceid"
			column="serviceid"
			type="java.lang.String"
			not-null="false"
			length="50"
		/>
		<!-- please tell Joe Hudson that the type 'nchar' could not be resolved.. defaulting to java.lang.String -->
		
		<!-- please tell Joe Hudson that the type 'nchar' could not be resolved.. defaulting to java.lang.String -->

		
	
		<property
			name="phone"
			column="phone"
			type="string"
			
		/>
		<property
			name="disnumber"
			column="disnumber"
			type="string"
			
		/>
		<property
			name="sname"
			column="sname"
			type="string"
			
		/>
		
		<property
			name="register"
			column="register"
			type="string"
			
		/>
<property
			name="jobname"
			column="jobname"
			type="string"
			
		/>
		<property
			name="remark"
			column="remark"
			type="string"
			
		/>
	</class>	
</hibernate-mapping>